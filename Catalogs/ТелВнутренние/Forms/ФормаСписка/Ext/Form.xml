	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT	Спр.Ссылка,Спр.Код,Спр.Сервер,ISNULL(Сост.Состояние,"") AS Состояние,
		CASE WHEN (Прив.Владелец IS NULL) THEN "" ELSE Прив.Владелец END AS Владелец,
		CASE WHEN (Прив.Владелец IS NULL) THEN "" ELSE Прив.Владелец.Холдер END AS Холдер,
		CASE WHEN (Прив.Владелец IS NULL) THEN "" ELSE Прив.Владелец.ТочкаРазмещения END AS ТочкаРазмещения,
		CASE WHEN (Конв.ТелМобильный IS NULL) THEN "" ELSE Конв.ТелМобильный.Код END AS ПараФМС
FROM	Справочник.ТелВнутренние Спр
LEFT	JOIN РегистрСведений.ОбъектыПривязка Прив ON (Прив.Объект = Спр.Ссылка)
LEFT	JOIN РегистрСведений.ТелКонвергенция Конв ON (Конв.ТелВнутренний = Спр.Ссылка)
LEFT	JOIN РегистрСведений.ТелСостояния Сост ON (Сост.Телефон = Спр.Ссылка)
</QueryText>
				<MainTable>Catalog.ТелВнутренние</MainTable>
