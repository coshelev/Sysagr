		<dataSource>ИсточникДанных1</dataSource>
		<query>SELECT	Период,КонтактЦентр,Область,Ресурс,1 AS Количество,
		CASE WHEN (Канал = ЗНАЧЕНИЕ(Справочник.ТелефоныВнешние.ПустаяСсылка)) THEN "Канал не определен" ELSE Канал END AS Канал
FROM	РегистрСведений.АнкетыВходящие
WHERE	(Период BETWEEN &amp;НачПериода AND &amp;КонПериода)</query>
