////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Анкеты.Телефон КАК Телефон,
	Анкеты.Вопрос КАК Вопрос,
	Анкеты.Сигнатура КАК Сигнатура,
	МАКСИМУМ(Анкеты.Период) КАК МаксПериод
ПОМЕСТИТЬ ВТ10_КлючиАнкеты
ИЗ
	РегистрСведений.АнкетыРасширенные КАК Анкеты
ГДЕ
	(Анкеты.Телефон, Анкеты.Сигнатура) В
			(ВЫБРАТЬ
				ВТ00.АбонентВнешний,
				ВТ00.Сигнатура
			ИЗ
				ВТ00_Звонки КАК ВТ00)
	И Анкеты.Вопрос В
			(ВЫБРАТЬ
				ВТ01_Вопрос.ВопросСсылка
			ИЗ
				ВТ01_Вопрос
		
			ОБЪЕДИНИТЬ
		
			ВЫБРАТЬ
				ВТ02_Вопрос1.ВопросСсылка
			ИЗ
				ВТ02_Вопрос1
		
			ОБЪЕДИНИТЬ
		
			ВЫБРАТЬ
				ВТ02_Вопрос2.ВопросСсылка
			ИЗ
				ВТ02_Вопрос2
		
			ОБЪЕДИНИТЬ
		
			ВЫБРАТЬ
				ВТ02_Вопрос3.ВопросСсылка
			ИЗ
				ВТ02_Вопрос3
		
			ОБЪЕДИНИТЬ
		
			ВЫБРАТЬ
				ВТ02_Вопрос4.ВопросСсылка
			ИЗ
				ВТ02_Вопрос4
		
			ОБЪЕДИНИТЬ
		
			ВЫБРАТЬ
				ВТ02_Вопрос5.ВопросСсылка
			ИЗ
				ВТ02_Вопрос5)

СГРУППИРОВАТЬ ПО
	Анкеты.Телефон,
	Анкеты.Вопрос,
	Анкеты.Сигнатура
;

